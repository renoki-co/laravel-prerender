{
    "name": "renoki-co/laravel-prerender",
    "description": "Prerender Laravel pages using Clusteer and this nice package.",
    "keywords": ["laravel", "php", "prerender", "ssr", "server", "side", "rendering", "render", "seo", "first", "contentful", "paint", "puppeteer", "clusteer"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-prerender",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^8.40",
        "illuminate/routing": "^8.40",
        "jenssegers/agent": "^2.6",
        "renoki-co/clusteer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelPrerender\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelPrerender\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.x-dev",
        "symfony/process": "^5.4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelPrerender\\LaravelPrerenderServiceProvider"
            ]
        }
    }
}
